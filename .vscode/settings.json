{
  "files.eol": "\n",
  "editor.formatOnSave": true,
  "eslint.lintTask.enable": true,
  "eslint.format.enable": true,
  "vetur.format.enable": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
